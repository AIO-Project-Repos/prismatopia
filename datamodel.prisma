type LambdaRole {
  id: ID! @id
  person: Person
  role: Role
}

type PeopleGroup {
  id: ID! @id
  name: String
  peopleGroupMember: [PeopleGroupMember]
}

type PeopleGroupMember {
  id: Int! @id(strategy: SEQUENCE) @sequence(name: "PeopleGroupMember_id_seq", initialValue: 1, allocationSize: 1)
  peopleGroup: PeopleGroup
  person: Person
}

type Person {
  id: ID! @id
  email: String
  firstname: String
  githubId: String
  lambdaRole: [LambdaRole]
  lastname: String
  peopleGroupMember: [PeopleGroupMember]
  productRole: [ProductRole]
  program: String
  projectRole: [ProjectRole]
  slackId: String
  timezone: String
}

type Product {
  id: ID! @id
  productName: String
  productRole: [ProductRole]
  project: [Project]
}

type ProductRole {
  id: ID! @id
  person: Person
  product: Product
  role: Role
}

type Project {
  id: ID! @id
  end: DateTime
  name: String
  product: Product
  projectGroupMember: [ProjectGroupMember]
  projectRole: [ProjectRole]
  start: DateTime
}

type ProjectGroup {
  id: ID! @id
  name: String
  projectGroupMember: [ProjectGroupMember]
}

type ProjectGroupMember {
  id: ID! @id
  project: Project
  projectGroup: ProjectGroup
}

type ProjectRole {
  id: ID! @id
  person: Person
  project: Project
  role: Role
}

type Role {
  id: ID! @id
  lambdaRole: [LambdaRole]
  productRole: [ProductRole]
  projectRole: [ProjectRole]
  roleName: String
  type: String
}
